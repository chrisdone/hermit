flatten-module
consider 'length
ww-split-param-unsafe 1 [| wrap |] [| unwrap |]
bash
any-call (unfold 'wrap)
any-call (unfold 'unwrap)
simplify
{ rhs-of 'work
  alpha-lam 'as
  lam-body
  push 'rep
  eta-expand 'acc
  lam-body
  case-float-app
  { [case-alt 0, alt-rhs]
    unfold 'rep
    unshadow
    case-inline-scrutinee
    case-elim
  }
  { case-alt 1
    alpha-alt ['b, 'bs]
    alt-rhs
    unfold-rule "rep-distr"
    one-td (foreward ww-result-fusion)
  }
}
one-td (unfold 'abs)

